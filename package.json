{
  "private": true,
  "scripts": {
    "prepare": "husky",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "vercel-build": "yarn run prebuild && next build && yarn run postbuild",
    "dev": "yarn run prebuild && next dev",
    "build": "yarn run prebuild && next build",
    "prebuild": "node scripts/generate-sw.mjs",
    "build:development": "NODE_ENV=development env-cmd -f .env.development yarn run prebuild && env-cmd -f .env.development next build",
    "start:development": "env-cmd -f .env.development next start",
    "build:staging": "NODE_ENV=staging env-cmd -f .env.staging yarn run prebuild && env-cmd -f .env.staging next build",
    "start:staging": "env-cmd -f .env.staging next start",
    "build:production": "NODE_ENV=production env-cmd -f .env.production yarn run prebuild && env-cmd -f .env.production next build",
    "start:production": "NODE_ENV=production env-cmd -f .env.production next start",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "scan": "npx react-scan@latest localhost:3000"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^1.0.6",
    "@sentry/nextjs": "^10.23.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.20",
    "cloudinary": "^2.7.0",
    "cssnano": "^7.0.6",
    "date-fns": "^2.29.3",
    "fast-xml-parser": "^4.4.0",
    "feed": "^4.2.2",
    "isomorphic-dompurify": "^2.25.0",
    "next": "16.0.1",
    "postcss": "^8.5.1",
    "react": "19.2.0",
    "react-datepicker": "^8.8.0",
    "react-dom": "19.2.0",
    "react-select": "^5.7.0",
    "react-share": "^5.0.3",
    "react-tooltip": "^5.26.0",
    "swr": "^2.2.5",
    "tailwindcss": "^3.3.6",
    "zod": "3.24.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "16.0.1",
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.1",
    "@types/react": "19.2.2",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.1",
    "cross-env": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.7",
    "next-sitemap": "^4.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^4.3.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@4.9.1",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "resolutions": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
